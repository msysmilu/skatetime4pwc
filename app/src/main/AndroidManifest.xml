<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="miluca.skatetime">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_skatetime5"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme">

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name="activities.AsapActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="activities.ProgramsActivity"

            android:label="Skating Activities">
        </activity>
        <activity
            android:name="activities.DatePickerActivity"
            android:label="Schedule">
        </activity>
		<activity
            android:name="activities.FilterActivity"
            android:label="Skate My Way">
        </activity>


    </application>

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest>
